      AssertEqualWithMessage ( "my log", callInfo.messageValue, "Logger.Log Call[" + IntegerFunctions:: ToString ( 3 ) + "]: Expected argument value 'message' is <" + "my log" + "> but was <" + callInfo.messageValue + ">" );


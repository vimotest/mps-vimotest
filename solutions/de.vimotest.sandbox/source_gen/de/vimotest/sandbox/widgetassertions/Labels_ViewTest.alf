  public then_Test_shows_text_My_Expectation_Text_and_is_enabled_and_is_visible (  ) {
    AssertEqualWithMessage ( "My Expectation Text", this.sut.getTestLabelText (  ), "Expected that label Test has text <" + "My Expectation Text" + ">, but was <" + this.sut.getTestLabelText (  ) + ">" );

